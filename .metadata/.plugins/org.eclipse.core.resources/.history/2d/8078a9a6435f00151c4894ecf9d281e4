
public class LinkedList<T> {
	public Node<T> head;
	public Node<T> tail;
	public int count;
	
	public LinkedList(){
		head = null;
		tail = null;
		count = 0;
	}
	
	public int getSize(){
		
		Node<T> temp = head;
		
		if(head == null){
			return count;
		}
		else{
			while(temp.getNextNode() != null){
				temp = temp.getNextNode();
				count++;
			}
		}
		return count;
	}
}
